@model List<SiteSakinleri>

<div class="container">
    <h1 class="text-center">Site Sakinleri Listesi</h1>
    <br />
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Ad Soyad</th>
                <th scope="col">Telefon Numarası</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var site_sakini in Model)
            {
                <tr>
                    <th scope="row">@site_sakini.AdSoyad</th>
                    <td>@site_sakini.Telefon</td>
                </tr>
            }
        </tbody>
    </table>
</div>
