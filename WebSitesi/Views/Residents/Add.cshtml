@model WebSitesi.Models.Ekleme.ResidentEkleViewModel

@* Test amaçlıdır. Projenin son halinde websitesinden ekleme işlemi yapılmıyor. *@

<form method="post">
	<div class="mt-3">
		<input type="text" name="Name" asp-for="Name" />
		<label for="Name">İsim</label>
	</div>
	<div class="mt-3">
		<input type="number" name="Phone" asp-for="Phone" />
		<label for="Phone">GSM</label>
	</div>
	<div class="mt-3">
		<button type="submit" class="btn btn-primary">Kaydet</button>
	</div>
</form>